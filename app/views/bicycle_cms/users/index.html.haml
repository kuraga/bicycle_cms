%table.users
  %tr
    %th= t('bicycle_cms/users.attributes.avatar')
    %th= t('bicycle_cms/users.attributes.name')
    %th= t('bicycle_cms/users.attributes.state')
    %th= t('bicycle_cms/users.attributes.email')
    %th= t('bicycle_cms/users.attributes.created_at')
  = render collection: @users, as: :user, partial: 'user_short'
